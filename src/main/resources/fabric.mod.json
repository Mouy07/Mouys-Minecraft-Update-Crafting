	"entrypoints": {
		"main": [
			"mouy.mmucrafting.MouysMinecraftUpdateCrafting"
		],
		"client": [
			"mouy.mmucrafting.MouysMinecraftUpdateCraftingClient"
		],
		"fabric-datagen": [
			"mouy.mmucrafting.MouysMinecraftUpdateCraftingDataGenerator"
		]
	},
	"mixins": [
		"mmucrafting.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
