{
	"schemaVersion": 1,
	"id": "mmucrafting",
	"version": "${version}",
	"name": "Mouy's Minecraft Update: Crafting",
	"description": "Overhauls a lot of crafting stuff.",
	"authors": [
		"Mouy"
	],
	"contact": {
		"sources": "https://github.com/Mouy07/Mouys-Minecraft-Update-Crafting/"
	},
	"license": "MIT",
	"icon": "assets/mmucrafting/icon.png",
